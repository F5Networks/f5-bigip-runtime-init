{
    "name": "f5-bigip-runtime-init",
    "version": "1.6.0",
    "release": "1",
    "description": "Cloud Onboarder for F5 BIG-IP",
    "author": "F5 Cloud Solutions",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/F5Networks/f5-bigip-runtime-init.git"
    },
    "scripts": {
        "audit": "npm audit --json > .auditReport.json; node scripts/auditProcessor.js",
        "sync-at-components-metadata": "bash scripts/sync_at_components_metadata.sh",
        "generate-readmes": "node ./scripts/render.js",
        "cli": "node ./src/cli.js",
        "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
        "build": "bash scripts/build_package.sh",
        "build-code-docs": "jsdoc -r src/lib -d ./code-docs/",
        "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' 'tests/**/*.{ts,tsx}'",
        "unit-tests": "mocha -r ts-node/register \"./tests/unit/**/*.ts\"",
        "test": "nyc mocha -r ts-node/register \"./tests/unit/**/*.ts\"",
        "functional-test": "mocha --recursive \"./tests/functional/testRunner.js\" --opts ./tests/functional/.mocha.opts",
        "check-test-coverage": "nyc check-coverage"
    },
    "resolutions": {
        "node-forge": "^1.3.0",
        "json-bigint": "^1.0.0",
        "minimist": "^1.2.6",
        "async": "^3.2.2",
        "ansi-regex": "^4.1.1",
        "qs": "^6.11.0",
        "jsonwebtoken": "^9.0.0"
    },
    "dependencies": {
        "@azure/identity": "^1.0.2",
        "@azure/keyvault-secrets": "^4.0.2",
        "@f5devcentral/f5-teem": "^1.5.0",
        "@google-cloud/compute": "^1.2.0",
        "@types/es6-promise": "^3.3.0",
        "ajv": "^6.12.2",
        "aws-sdk": "^2.610.0",
        "aws4": "^1.12.0",
        "commander": "^4.1.0",
        "get-user-locale": "^1.4.0",
        "google-auth-library": "^6.1.1",
        "googleapis": "^61.0.0",
        "icrdk": "git+https://github.com/f5devcentral/f5-icontrollx-dev-kit#master",
        "jmespath": "^0.15.0",
        "js-yaml": "^3.13.1",
        "lodash": "^4.17.21",
        "lodash.where": "^3.1.0",
        "mustache": "^4.0.0",
        "netmask": "^2.0.2",
        "request": "^2.88.2",
        "uuid": "^8.2.0",
        "winston": "^3.3.3",
        "xmldom": ">= 0.5.x",
        "y18n": "4.0.1"
    },
    "dependencyMap": {
        "common": {
            "commander": "^4.1.0",
            "winston": "^3.3.3",
            "get-user-locale": "^1.4.0",
            "uuid": "^8.2.0",
            "@f5devcentral/f5-teem": "^1.4.6",
            "js-yaml": "^3.13.1",
            "mustache": "^4.0.0",
            "request": "^2.88.0",
            "jmespath": "^0.15.0",
            "netmask": "^2.0.2"
        },
        "gcp": {
            "google-auth-library": "^5.6.1",
            "googleapis": "^48.0.0",
            "@google-cloud/compute": "^1.2.0"
        },
        "aws": {
            "aws-sdk": "^2.610.0",
            "aws4": "^1.12.0",
            "lodash.where": "^3.1.0"
        },
        "azure": {
            "@azure/keyvault-secrets": "4.3.0",
            "@azure/identity": "1.4.0",
            "lodash.where": "^3.1.0"
        }
    },
    "devDependencies": {
        "@azure/ms-rest-nodeauth": "^3.0.1",
        "@f5devcentral/eslint-config-f5-atg": "^0.1.4",
        "@types/assert": "^1.4.6",
        "@types/memory-cache": "^0.2.1",
        "@types/mocha": "^7.0.2",
        "@types/node": "^13.11.0",
        "@types/sinon": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-plugin-import": "^2.20.0",
        "jsdoc": "^3.6.10",
        "json-schema-md-doc": "^0.1.7",
        "mocha": "^7.1.2",
        "mock-fs": "^4.11.0",
        "nock": "^11.7.2",
        "nyc": "^14.1.1",
        "q": "^1.5.1",
        "sinon": "^8.1.1",
        "ts-node": "^8.8.2",
        "typescript": "^3.8.3"
    },
    "mocha": {
        "timeout": 5000,
        "exit": true
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "tests/**"
        ],
        "check-coverage": true,
        "statements": 90,
        "branches": 85,
        "functions": 90,
        "lines": 90,
        "reporter": [
            "html",
            "text"
        ],
        "cache": false
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "extends": [
            "plugin:@typescript-eslint/recommended"
        ]
    },
    "auditProcessor": {
        "whitelist": [
