    },
    "keywords": [
        "f5",
        "node",
        "javascript",
        "typescript"
    ],
    "tags": [
        "f5",
        "f5-bigip-runtime-init",
        "node",
        "javascript",
        "typescript"
    ],
    "bugs": {
        "url": "https://github.com/F5Networks/f5-bigip-runtime-init.git/issues"
    }
}
